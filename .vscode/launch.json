    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run app (debug mode)",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define-from-file",
                "env.env",
            ]
        },
        {
            "name": "Run app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define-from-file",
                "env.env",
            ]
        },
        {
            "name": "Run app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define-from-file",
                "env.env",
            ]
        }
    ]
}
